{% style %}
  @font-face { 
    font-family: 'Sofia Pro Bold';
    src: url(https://cdn.shopify.com/s/files/1/0597/0545/8799/files/Sofia_Pro_Bold_Az.woff?v=1725429184) format('woff');
    font-weight: 700;
    font-style: normal;
  }
  strong {
    font-weight: 700;
  }
  body, p, a {
    font-family: 'Sofia Pro Regular' !important;
  }
  @font-face { 
    font-family: 'Sofia Pro Regular';
    src: url(https://cdn.shopify.com/s/files/1/0597/0545/8799/files/Sofia_Pro_Regular_Az.woff?v=1725534934) format('woff');
  }
  @font-face { 
    font-family: 'Nord Regular';
    src: url(https://cdn.shopify.com/s/files/1/0597/0545/8799/files/NORD-Regular.woff?v=1725429572) format('woff');
  }
  @font-face { 
    font-family: 'Nord Bold';
    src: url(https://cdn.shopify.com/s/files/1/0597/0545/8799/files/nord-bold_2.ttf?v=1732876667) format('truetype');
  }
  @font-face {
    font-family: 'Nord Medium';
    src: url(https://cdn.shopify.com/s/files/1/0597/0545/8799/files/nord-medium.ttf?v=1732886217) format('truetype');
  }
  @font-face {
    font-family: 'Sofia Pro SemiBold';
    src: url(https://cdn.shopify.com/s/files/1/0597/0545/8799/files/sofia-pro-semibold-webfont.woff?v=1733151152) format('woff');
  }
  @font-face {
    font-family: 'Sofia Pro Medium';
    src: url(https://cdn.shopify.com/s/files/1/0597/0545/8799/files/Sofia_Pro_Medium.woff?v=1733154689) format('woff');
  }
  .hero-text .custom-heading, .custom-heading, h1, h2, h3, h4, h5, .h0, .h1, .h2, .h3, .h4, .h5 { font-family: 'Nord Regular';}
{% endstyle %}
