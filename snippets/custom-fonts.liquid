    font-family: 'Instrument Sans';
    src: url(https://cdn.shopify.com/s/files/1/0597/0545/8799/files/InstrumentSans-VariableFont_wdth_wght.ttf?v=1733156956) format('truetype');
  }
  .hero-text .custom-heading, .custom-heading, h1, h2, h3, h4, h5, .h0, .h1, .h2, .h3, .h4, .h5 { font-family: 'Nord Regular';}
